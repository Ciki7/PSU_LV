1.Zadatak

import numpy as np
import matplotlib.pyplot as plt
import os

x = np.array([1, 2, 3, 3, 1])
y = np.array([1, 2, 2, 1, 1])
plt.plot(x, y, linewidth=2, marker="o", markersize=8, color="blue", linestyle= "-")

plt.axis([0,4,0,4])
plt.xlabel("x os")
plt.ylabel("y os")
plt.title("primjer")
plt.show()

